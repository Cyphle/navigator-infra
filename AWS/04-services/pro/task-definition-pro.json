{
    "cpu": "${TASK_CPU}",
    "memory": "${TASK_MEMORY}",
    "volumes": [],
    "networkMode": "awsvpc",
    "family": "${PRODUCT}-${APPLICATION}-${ENVIRONMENT}",
    "requiresCompatibilities": ["FARGATE"],
    "placementConstraints": [],
    "executionRoleArn": "arn:aws:iam::${AWS_ACCOUNT_ID}:role/${PRODUCT}-${APPLICATION}-ecs-taskexec-${AWS_REGION}-${ENVIRONMENT}",
    "taskRoleArn": "arn:aws:iam::${AWS_ACCOUNT_ID}:role/${PRODUCT}-${APPLICATION}-ecs-task-${AWS_REGION}-${ENVIRONMENT}",
    "containerDefinitions": [
      {
        "name": "${APPLICATION}",
        "cpu": ${TASK_CPU},
        "memory": ${TASK_MEMORY},
        "linuxParameters": {
          "initProcessEnabled": true
        },
        "essential": true,
        "volumesFrom": [],
        "mountPoints": [],
        "image": "",
        "portMappings": [
          {
            "protocol": "tcp",
            "containerPort": ${API_PORT},
            "hostPort": ${API_PORT}
          }
        ],
        "environment": [
          {
            "name": "AWS_REGION",
            "value": "${AWS_REGION}"
          },
          {
            "name": "ENV",
            "value": "${ENVIRONMENT}"
          },
          {
            "name": "env",
            "value": "${ENVIRONMENT}"
          },
          {
            "name": "PORT",
            "value": "${API_PORT}"
          }
        ],
        "secrets": [
          {
            "name": "CC_JAVA_VERSION",
            "valueFrom": "${LITE_PRO_APP_CONFIG_SECRET_ARN}:CC_JAVA_VERSION::"
          },
          {
            "name": "AIRTABLE_ACCESS_TOKEN",
            "valueFrom": "${LITE_PRO_APP_CONFIG_SECRET_ARN}:AIRTABLE_ACCESS_TOKEN::"
          },
          {
            "name": "BREVO_API_KEY",
            "valueFrom": "${LITE_PRO_APP_CONFIG_SECRET_ARN}:BREVO_API_KEY::"
          },
          {
            "name": "PRO_API_AUTHORIZATION_HEADER",
            "valueFrom": "${LITE_PRO_APP_CONFIG_SECRET_ARN}:PRO_API_AUTHORIZATION_HEADER::"
          },
          {
            "name": "MAIL_DEV_DESTINATION",
            "valueFrom": "${LITE_PRO_APP_CONFIG_SECRET_ARN}:MAIL_DEV_DESTINATION::"
          },
          {
            "name": "MAIL_DEV_LOG_SENDER",
            "valueFrom": "${LITE_PRO_APP_CONFIG_SECRET_ARN}:MAIL_DEV_LOG_SENDER::"
          },
          {
            "name": "INTERNAL_ENEDIS_COLLECTOR_SECURITY_TOKEN",
            "valueFrom": "${LITE_COMMON_APP_CONFIG_SECRET_ARN}:INTERNAL_ENEDIS_COLLECTOR_SECURITY_TOKEN::"
          },
          {
            "name": "INTERNAL_API_SECURITY_TOKEN",
            "valueFrom": "${LITE_COMMON_APP_CONFIG_SECRET_ARN}:INTERNAL_API_SECURITY_TOKEN::"
          },
          {
            "name": "INTERNAL_PARTNER_SECURITY_TOKEN",
            "valueFrom": "${LITE_COMMON_APP_CONFIG_SECRET_ARN}:INTERNAL_PARTNER_SECURITY_TOKEN::"
          },
          {
            "name": "INTERNAL_PRICING_SECURITY_TOKEN",
            "valueFrom": "${LITE_COMMON_APP_CONFIG_SECRET_ARN}:INTERNAL_PRICING_SECURITY_TOKEN::"
          },
          {
            "name": "INTERNAL_PRO_SECURITY_TOKEN",
            "valueFrom": "${LITE_COMMON_APP_CONFIG_SECRET_ARN}:INTERNAL_PRO_SECURITY_TOKEN::"
          },
          {
            "name": "INTERNAL_WEB_SECURITY_TOKEN",
            "valueFrom": "${LITE_COMMON_APP_CONFIG_SECRET_ARN}:INTERNAL_WEB_SECURITY_TOKEN::"
          },
          {
            "name": "NEW_RELIC_API_KEY",
            "valueFrom": "${LITE_COMMON_APP_CONFIG_SECRET_ARN}:NEW_RELIC_API_KEY::"
          },
          {
            "name": "POSTGRESQL_ADDON_DB",
            "valueFrom": "${DB_CONFIG_SECRET_ARN}:dbname::"
          },
          {
            "name": "POSTGRESQL_ADDON_HOST",
            "valueFrom": "${DB_CONFIG_SECRET_ARN}:host::"
          },
          {
            "name": "POSTGRESQL_ADDON_PASSWORD",
            "valueFrom": "${DB_CONFIG_SECRET_ARN}:password::"
          },
          {
            "name": "POSTGRESQL_ADDON_PORT",
            "valueFrom": "${DB_CONFIG_SECRET_ARN}:port::"
          },
          {
            "name": "POSTGRESQL_ADDON_USER",
            "valueFrom": "${DB_CONFIG_SECRET_ARN}:username::"
          },
          {
            "name": "DATAWAREHOUSE_DB",
            "valueFrom": "${DB_DATAWAREHOUSE_CONFIG_SECRET_ARN}:dbname::"
          },
          {
            "name": "DATAWAREHOUSE_HOST",
            "valueFrom": "${DB_DATAWAREHOUSE_CONFIG_SECRET_ARN}:host::"
          },
          {
            "name": "DATAWAREHOUSE_PASSWORD",
            "valueFrom": "${DB_DATAWAREHOUSE_CONFIG_SECRET_ARN}:password::"
          },
          {
            "name": "DATAWAREHOUSE_POSTGRESQL_PASSWORD",
            "valueFrom": "${DB_DATAWAREHOUSE_CONFIG_SECRET_ARN}:password::"
          },
          {
            "name": "DATAWAREHOUSE_PORT",
            "valueFrom": "${DB_DATAWAREHOUSE_CONFIG_SECRET_ARN}:port::"
          },
          {
            "name": "DATAWAREHOUSE_USER",
            "valueFrom": "${DB_DATAWAREHOUSE_CONFIG_SECRET_ARN}:username::"
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/fargate/service/${PRODUCT}-${APPLICATION}-${ENVIRONMENT}",
            "awslogs-region": "${AWS_REGION}",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ]
  }
  